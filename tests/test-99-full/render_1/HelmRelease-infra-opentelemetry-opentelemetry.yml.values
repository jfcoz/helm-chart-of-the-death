admissionWebhooks:
  certManager:
    enabled: true
  # Exclude system namespaces, cf https://cloud.google.com/kubernetes-engine/docs/how-to/optimize-webhooks?hl=fr#unsafe-webhooks
  namespaceSelector:
    matchExpressions:
      - key: kubernetes.io/metadata.name
        operator: NotIn
        values:
          - kube-system
          - kube-node-lease
manager:
  collectorImage:
    repository: "ghcr.io/open-telemetry/opentelemetry-collector-releases/opentelemetry-collector-k8s"
  serviceMonitor:
    enabled: true
  prometheusRule:
    enabled: true
  resources:
    requests:
      cpu: 5m
    limits:
      cpu: 500m
      #extraArgs:
      #  - --feature-gates=operator.autoinstrumentation.go
