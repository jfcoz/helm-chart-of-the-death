admissionController:
  container:
    resources:
      limits:
        memory: 150M
      requests:
        cpu: 100m
        memory: 150M
  networkPolicy:
    enabled: true
  replicas: 3
  serviceMonitor:
    enabled: true
  tolerations:
    - effect: NoSchedule
      key: storage
      operator: Exists
  topologySpreadConstraints:
    - labelSelector:
        matchLabels:
          app.kubernetes.io/component: admission-controller
          app.kubernetes.io/instance: all-in-one
      maxSkew: 1
      topologyKey: topology.kubernetes.io/zone
      whenUnsatisfiable: DoNotSchedule
backgroundController:
  resources:
    requests:
      cpu: 1m
cleanupController:
  resources:
    requests:
      cpu: 1m
config:
  webhooks:
    namespaceSelector:
      matchExpressions:
        - key: kubernetes.io/metadata.name
          operator: NotIn
          values:
            - kube-system
            - kube-node-lease
features:
  omitEvents:
    eventTypes:
      - PolicyApplied
reportsController:
  resources:
    limits:
      memory: 200Mi
    requests:
      cpu: 1m
      memory: 100Mi
test:
  sleep: 1
